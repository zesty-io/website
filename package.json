{
  "name": "zesty-website",
  "author": "Zesty.io Platform Inc.",
  "email": "marketing@zesty.io",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "prettier:fix": "prettier './src/**/*.js' --write",
    "clear-all": "rimraf ./node_modules ./.next",
    "sync": "node src/lib/sync.js",
    "docker-build": "docker build .",
    "gcloud-build": "gcloud builds submit --tag gcr.io/zesty-dev/zesty-website:1.0.0  --project zesty-dev",
    "gcloud-deploy": "gcloud run deploy zesty-website --image=gcr.io/zesty-dev/zesty-website:1.0.0 --platform managed --port 3000 --project zesty-dev --allow-unauthenticated --min-instances 1 --region us-central1",
    "deploy-stage": "npm run gcloud-build && npm run gcloud-deploy",
    "prepare:dev": "chmod ug+x .husky/*",
    "prepare": "husky install && npm run prepare:dev",
    "test": "jest --forceExit",
    "dev-test": "jest --watch --silent"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not dead",
      "not IE 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.1",
    "@mui/material": "^5.6.1",
    "@zesty-io/live-editor": "^2.0.22",
    "aos": "^2.3.4",
    "card-validator": "^8.1.1",
    "cookies-next": "^2.0.4",
    "formik": "^2.2.6",
    "fuse.js": "^6.6.2",
    "himalaya": "^1.1.0",
    "jarallax": "^1.12.8",
    "markdown-it": "^13.0.1",
    "markdown-to-jsx": "^7.1.7",
    "mui-markdown": "^0.5.3",
    "next": "^12.1.4",
    "node-fetch": "^2.6.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-confetti": "^6.1.0",
    "react-countup": "^6.2.0",
    "react-dom": "^17.0.1",
    "react-fast-marquee": "^1.3.4",
    "react-image-lightbox": "^5.1.4",
    "react-lazy-load-image-component": "^1.5.4",
    "react-player": "^2.10.0",
    "react-slick": "^0.28.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-visibility-sensor": "^5.1.1",
    "react-window": "^1.8.7",
    "shortid": "^2.2.16",
    "slick-carousel": "^1.8.1",
    "swiper": "^8.3.2",
    "tape": "^5.5.3",
    "tape-async": "^2.3.0",
    "yup": "^0.32.11",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "colortape": "^0.1.2",
    "cross-env": "^7.0.2",
    "husky": "^8.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run build"
    }
  }
}
