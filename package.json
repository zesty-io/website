    "react-text-transition": "^3.1.0",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.9",
    "remark-gfm": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "sweetalert2": "^11.7.31",
    "sweetalert2-react-content": "^5.0.7",
